transition: all 0.3s ease 0s;\n    background-color: rgb(12, 108, 242);\n    color: rgb(255, 255, 255);\n}\n.btnLogin{\n    margin-left: 1.4rem;\n    display: inline-flex;\n    justify-content: center;\n    align-items: center;\n    padding: 0.75rem 1.375rem;\n    font-size: 1.125rem;\n    font-weight: 500;\n    letter-spacing: 0.01em;\n    line-height: 144%;\n    border-radius: 0.5rem;\n    border: 0.125rem solid transparent;\n    cursor: pointer;\n    transition: all 0.3s ease 0s;\n    color: rgb(18, 29, 51);\n    background: rgb(255, 255, 255);\n}\n\n.valueContainer{\n    box-sizing: content-box;\n    max-width: 62.5rem;\n    margin: 0px auto;\n    padding: 0px 2.5rem;\n}\n.valueContainerWrapper{\n    position: relative;\n    display: flex;\n    -webkit-box-pack: center;\n    justify-content: center;\n    width: 100%;\n    margin-top: -70px;\n}\n.valueContainerContent{\n    display: flex;\n    flex-direction: column;\n    align-items: flex-end;\n    max-width: 62.5rem;\n    width: 100%;\n}\n.valueWrapper{\n    width: 100%;\n}\n.values{\n        display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(12rem, auto));\n    gap: 1.5rem;\n    border-radius: 0.5rem;\n    width: 100%;\n    margin-bottom: 1.25rem;\n}\n.bitCoin{\n    background-image: linear-gradient(rgb(255, 255, 255), rgb(255, 255, 255));\n    border-radius: 0.5rem;\n    box-shadow: rgb(103 113 133 / 20%) 0.75rem 1.5rem 4rem 0rem;\n    color: rgb(18, 29, 51);\n    min-width: 0px;\n    padding: 1.25rem;\n    transition: background-image 0.6s ease 0s;\n}\n.header{\n    display: flex;\n    align-items: flex-start;\n    justify-content: space-between;\n    margin-bottom: 0.5rem;\n}\n.iconContainer{\n    flex-shrink: 0;\n}\n.imgContainer{\n    height: 2.5rem;\n    width: 2.5rem;\n}\n.priceContainer{\n    display: flex;\n    align-items: flex-start;\n    -webkit-box-pack: end;\n    justify-content: flex-end;\n    flex-wrap: wrap;\n    max-width: 170px;\n}\n.btnBuy{\n    background-color: rgb(239, 236, 254);\n    border-radius: 0.25rem;\n    border: none;\n    color: rgb(83, 34, 229);\n    cursor: pointer;\n    font-weight: 500;\n    margin: 0px 0px 0.5rem 0.5rem;\n    padding: 0.25rem 0.5rem;\n    font-size: 0.875rem;\n    line-height: 1.25rem;\n    transition: background-color 0.3s ease 0s;\n}\n.btnTrade{\n    background-color: rgb(230, 250, 236);\n    color: rgb(0, 153, 76);\n}\n.priceCoinNameContent{\n    display: flex;\n    margin-bottom: 0.25rem;\n}\n.priceCoinNameContent p{\n    font-weight: 500;\n}\n.coinValue{\n    color: rgb(103, 113, 133);\n    margin-left: 0.5rem;\n}\n.priceInfo{\n    display: flex;\n}\n.price{\n    font-weight: 500;\n    font-size: 1rem;\n    /* line-height: 1.5rem; */\n}\n.pricePercent{\n    margin-left: 0.5rem;\n    color: red;\n    font-weight: 500;\n}\n.viewMore{\n    color: white;\n}"},"lineCount":null}},"error":null,"hash":"673313c085381a86849d52f97ab751d8","cacheData":{"env":{}}}
106,"column":2},"source":"components/Block.js","original":{"line":4,"column":20}},{"name":"Block","generated":{"line":108,"column":15},"source":"components/Block.js","original":{"line":79,"column":15}}],"sources":{"components/Block.js":"import React , {Component} from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\nclass Block extends Component{\n    state = {displayTransaction:false};\n\n    toggleTransaction = () =>{\n        this.setState({displayTransaction: !this.state.displayTransaction});\n    }\n\n    get displayTransaction (){\n        const {data} = this.props.block;\n\n        const stringifiedData = JSON.stringify(data);\n\n        const dataDisplay = stringifiedData.length>35 ? `${stringifiedData.substring(0,35)}...`: stringifiedData;\n\n        if(this.state.displayTransaction){\n            return(\n                <div>\n                    {\n                        data.map(transaction =>(\n                            <div key={transaction.id}>\n                                <hr/>\n                                <Transaction transaction={transaction}/>\n                            </div>\n                        ))\n                    }\n                    <br/>\n                    <Button\n                    variant=\"primary\"\n                    onClick={this.toggleTransaction}\n                    >\n                        Show Less\n                    </Button>\n                </div>\n            )\n        }\n\n        return (\n            <div>\n            <div>\n                Data: {dataDisplay}\n            </div>\n            <Button \n            variant=\"danger\"\n            onClick={this.toggleTransaction}\n            >\n                Show More\n            </Button>\n            </div>\n        )\n    }\n\n    render(){\n\n        // console.log('this.displayTransaction', this.displayTransaction);\n\n        const {timestamp, hash , data} = this.props.block;\n        const hashDisplay = `${hash.substring(0,15)}...`;\n       \n\n        return(\n            <div className=\"blockMap\">\n                <div>\n                    Hash: {hashDisplay}\n                </div>\n                <div>\n                    Timestamp : {new Date(timestamp).toLocaleString()}\n                </div>\n              \n                {this.displayTransaction}\n\n            </div>\n        )\n    }\n}\n\nexport default Block;"},"lineCount":null}},"error":null,"hash":"3ddc165c40970ccfc24ad9430ca3684b","cacheData":{"env":{}}}
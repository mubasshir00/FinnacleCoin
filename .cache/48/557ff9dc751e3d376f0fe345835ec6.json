e":115,"column":0},"source":"components/TransactionPool.js","original":{"line":64,"column":5}},{"name":"Component","generated":{"line":119,"column":2},"source":"components/TransactionPool.js","original":{"line":9,"column":30}},{"name":"TransactionPool","generated":{"line":121,"column":15},"source":"components/TransactionPool.js","original":{"line":67,"column":15}}],"sources":{"components/TransactionPool.js":"import React, { Component } from 'react'\nimport { Button } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport Transaction from './Transaction';\nimport history from '../history'\n\nconst POLL_INERVAL_MS = 10000;\n\nclass TransactionPool extends Component {\n    state = {transactionPoolMap:{}}\n    fetchTransactionPoolMap = () =>{\n        fetch(`${document.location.origin}/api/transaction-pool-map`)\n        .then(response=>response.json())\n        .then(json=>this.setState({transactionPoolMap:json}));\n    }\n    componentDidMount(){\n        this.fetchTransactionPoolMap();\n\n        this.fetchPoolMapInterval =setInterval(\n            () => this.fetchTransactionPoolMap(),\n            POLL_INERVAL_MS\n        );\n    }\n    componentWillUnmount(){\n        clearInterval(this.fetchPoolMapInterval);\n    }\n    fetchMineTransactions = () =>{\n        fetch(`${document.location.origin}/api/mine-transaction`)\n           .then(response=>{\n               if(response.status===200){\n                   alert('success');\n                   history.push('/blocks');\n               } else{\n                   alert('The mine-transactions block request did not complete.');\n               }\n           })\n    }\n    render(){\n        return(\n            <div>\n                <div>\n                    <Link to=\"/\">Home</Link>\n                    <h3>Transaction Pool</h3>\n                    {\n                        Object.values(this.state.transactionPoolMap).map(transaction=>{\n                            return (\n                                <div key={transaction.id}>\n                                    <hr/>\n                                    <Transaction transaction={transaction}/>\n                                </div>\n                            )\n                        })\n                    }\n                </div>\n                <hr/>\n                <Button\n                variant=\"primary\"\n                onClick = {this.fetchMineTransactions}\n                >\n                    Mine The Transactions\n                </Button>\n            </div>\n        )\n    }\n}\n\nexport default TransactionPool\n"},"lineCount":null}},"error":null,"hash":"9b4f0a83eab8e2e9b4cfc4290a2c3512","cacheData":{"env":{}}}
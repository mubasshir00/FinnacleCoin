ing: content-box;\n    margin: 0px auto;\n    padding: 0px 2.5rem;\n}\n.featureSection{\n    border-radius: 1.25rem;\n    color: rgb(18, 29, 51);\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    background-color: rgba(239, 236, 254, 0.5);\n    background-image: -webkit-image-set(url('https://www.blockchain.com/static/img/home/home-wallet-shapes-bg.png') 1x, url('https://www.blockchain.com/static/img/home/home-wallet-shapes-bg@2x.png') 2x);\n    background-repeat: no-repeat;\n    background-size: 625px;\n    background-position: right bottom 2.5rem;\n    min-height: 52.5rem;\n    margin: 3.75rem auto;\n    max-width: 80rem;\n}\n.leftCol ,.exchangeFeatureRight{\n    color: rgb(115, 73, 242);\n    /* -webkit-box-pack: start; */\n    justify-content: flex-start;\n    padding-top: 70px;\n    padding-bottom: 70px;\n\n    display: flex;\n    flex-direction: column;\n    /* justify-content: center; */\n    padding-left: 8.75rem;\n    flex-shrink: 0;\n}\n.leftCol h2 , .exchangeFeatureRight h2{\n    color: inherit;\n    margin: 0px 0px 1.875rem;\n    font-size: 2.5rem;\n    font-weight: 600;\n    line-height: 3.25rem;\n}\n.featuresContent{\n        margin-bottom: 1.875rem;\n    max-width: 30.5rem;\n}\n.featuresContentWrapper{\n    border-radius: 0.75rem;\n    border: 0.0625rem solid rgb(222, 216, 253);\n    margin-bottom: 0.3rem;\n}\n.leftCol h3 , .exchangeFeatureRight h3{\n        color: rgb(115, 73, 242);\n    font-size: 1rem;\n    font-weight: 500;\n    line-height: 1.5rem;\n    margin: 0px;\n    padding: 1rem 1.25rem;\n    transition: color 0.25s ease 0s;\n    z-index: 1;\n    cursor: pointer;\n}\n.leftCol p , .exchangeFeatureRight p{\n        /* max-height: 0px; */\n    max-width: 80%;\n    /* opacity: 0; */\n    overflow: hidden;\n    padding: 0px 1.25rem;\n    transition: all 0ms ease 0s;\n}\n.getStarted{\n        display: inline-flex;\n    justify-content: center;\n    align-items: center;\n    padding: 0.75rem 1.375rem;\n    font-size: 1.125rem;\n    font-weight: 500;\n    letter-spacing: 0.01em;\n    line-height: 144%;\n    border-radius: 0.5rem;\n    border: 0.125rem solid transparent;\n    cursor: pointer;\n    transition: all 0.3s ease 0s;\n    color: rgb(255, 255, 255);\n    background: rgb(115, 73, 242);\n    width: 50%;\n}\n\n.rightCol{\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    padding-left: 5rem;\n    flex-shrink: 0;\n}\n\n.rightColImageWrapper{\n    height: 100%;\n    width: 100%;\n    position: relative;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.rightColImage{\n    position: absolute;\n}\n\n.exchangeSection{\n    overflow: hidden;\n}\n.exachangeSectionContainer{\n    max-width: 80rem;\n    box-sizing: border-box;\n    margin: 0px auto;\n    padding: 0px 2.5rem;\n}\n.exchangeFeature{\n        border-radius: 1.25rem;\n    color: rgb(18, 29, 51);\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    background-color: rgba(239, 236, 254, 0.5);\n    background-image: -webkit-image-set(url('https://www.blockchain.com/static/img/home/home-exchange-shapes-bg.png') 1x, url('https://www.blockchain.com/static/img/home/home-exchange-shapes-bg@2x.png') 2x);\n    background-repeat: no-repeat;\n    background-size: 625px;\n    /* background-position: right bottom 2.5rem; */\n    min-height: 52.5rem;\n    margin: 3.75rem auto;\n    max-width: 80rem;\n}\n.exachngeLeft{\n        display: flex;\n    flex-direction: column;\n    -webkit-box-pack: center;\n    justify-content: center;\n    padding-left: 8.75rem;\n    flex-shrink: 0;\n}\n.leftWrapper{\n    height: 100%;\n    width: 100%;\n    position: relative;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n.exchangeImg{\n    position: absolute;\n}\n\n.exchangeFeatureRight{\n        -webkit-box-pack: start;\n    justify-content: flex-start;\n    padding-top: 100px;\n    padding-bottom: 100px;\n        display: flex;\n    flex-direction: column;\n    /* -webkit-box-pack: center; */\n    /* justify-content: center; */\n    padding-left: 5rem;\n    flex-shrink: 0;\n}\n\n.banner{\n    background-color: rgb(18, 29, 51);\n    background-image: url('https://www.blockchain.com/static/img/banner/banner-bg.svg');\n    background-repeat: no-repeat;\n    background-size: 1440px;\n    background-position: center top;\n    color: rgb(255, 255, 255);\n    padding: 80px 0px;\n}\n.bannerWrapper{\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n.bannerWrapper h2{\n        font-size: 2.5rem;\n    font-weight: 600;\n    line-height: 3.25rem; \n        margin: 0px 32px 0px 0px;\n\n}\n.bannerWrapper .link{\n        display: inline-flex;\n    -webkit-box-pack: center;\n    justify-content: center;\n    -webkit-box-align: center;\n    align-items: center;\n    padding: 0.75rem 1.375rem;\n    font-size: 1.125rem;\n    font-weight: 500;\n    letter-spacing: 0.01em;\n    line-height: 144%;\n    border-radius: 0.5rem;\n    border: 0.125rem solid transparent;\n    cursor: pointer;\n    transition: all 0.3s ease 0s;\n    color: rgb(12, 108, 242);\n    background: rgb(255, 255, 255);\n}"},"lineCount":206}},"error":null,"hash":"dc871c34144cd2bb09d6a769c095fa75","cacheData":{"env":{}}}